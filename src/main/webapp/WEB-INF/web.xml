   <!-- Define the Servlet -->
   
     <servlet>
        <servlet-name>DemoServlet</servlet-name>
        <servlet-class>com.sanzida1.DemoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DemoServlet</servlet-name>
        <url-pattern>/DemoServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>square</servlet-name>
        <servlet-class>com.sanzida.SquareServlet</servlet-class>
    </servlet>

    <!-- Map the Servlet to a URL pattern -->
    <servlet-mapping>
        <servlet-name>square</servlet-name>
        <url-pattern>/sq</url-pattern>
    </servlet-mapping>
    
    
    
    
    <servlet>
        <servlet-name>my</servlet-name>
        <servlet-class>com.sanzida.myServlet</servlet-class>
        
        <init-param>
	        <param-name>name</param-name>
	        <param-value>sanzida</param-value>
        </init-param>
    </servlet>

    <!-- Map the Servlet to a URL pattern -->
    <servlet-mapping>
        <servlet-name>my</servlet-name>
        <url-pattern>/servlet</url-pattern>
    </servlet-mapping>
    
    
    
    <context-param>
	    <param-name>name</param-name>
	    <param-value>nitu</param-value>
    </context-param>
    
    
    <context-param>
	    <param-name>phone</param-name>
	    <param-value>samsung</param-value>
    </context-param>
    
    
    
    
    
    
    
    
    
    
    


</web-app>
